"Motor Conveyer"                :"%QX0.0 & %QX0.1",
"Gripper rotate Height"         :"%QX10.0",
"Gripper rotate Weight"         :"%QX10.1",
"Gripper open / close"          :"%QX10.4",
"Height cilinder"               :"%QX10.6",
"Gripper omhoog/omlaag"         :"%QX11.0",
"Load sensor"                   :"%IW1",
"Sensor conveyer at gripper"    :"%IX0.0",
"Sensor gripper open / close"   :"%IX0.1",
"Sensor gripper up / down"      :"%IX0.2",
"Sensor gripper at height"      :"%IX0.3",
"Sensor gripper at weight"      :"%IX0.4",
"Sensor at conveyer"            :"%IX0.5",
"Sensor conveyer at RFID reader":"%IX0.6"
}
